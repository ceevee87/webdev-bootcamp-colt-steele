
declare -a dirs=(api bin collections config controllers env lib models public public/assets public/assets/css public/assets/js public/assets/js/lib routes test views)

for dir in "${dirs[@]}"
do
   if [ ! -d $dir ]; then mkdir $dir; fi
done

declare -a files=(app.js README.md)
for ff in "${files[@]}"
do
   if [ ! -e $ff ]; then touch $ff; fi
done

